if &syntax != 'scheme'
setlocal syntax=scheme
endif
setlocal tabstop=8
setlocal tags=
setlocal textwidth=68
setlocal thesaurus=
setlocal nowinfixheight
setlocal wrap
setlocal wrapmargin=0
silent! normal! zE
let s:l = 36 - ((30 * winheight(0) + 17) / 35)
if s:l < 1 | let s:l = 1 | endif
exe s:l
normal! zt
